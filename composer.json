{
    "name": "gurucomkz/rel-picker",
    "description": "Relation Picker field for SilverStripe",
    "license": "BSD-3-Clause",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "tag",
        "field"
    ],
    "authors": [
        {
            "name": "Sergey Shevchenko",
            "homepage": "http://github.com/gurucomkz"
        }
    ],
    "require": {
        "silverstripe/admin": "^1.7@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Gurucomkz\\RelPickerField\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        },
        "expose": [
            "client/dist"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
